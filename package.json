	"version": "1.0.0",
	"description": "creating a nodejs server with babel 7 and webpack 4",
	"main": "./src/index.js",
	"browserslist": [
		">1%",
		"last 4 versions",
		"ie >= 11",
		"edge >= 16",
		"firefox >= 43",
		"Firefox ESR",
		"chrome >= 47",
		"ChromeAndroid >= 69.0",
		"safari >= 11"
	],
	"scripts": {
		"test": "test",
		"start": "nodemon",
